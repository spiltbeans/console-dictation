		"sysadmin",
		"syslog",
		"tools",
		"persistent",
		"persistent logs",
		"node"
	],
	"author": "Eyas Valdez (https://github.com/spiltbeans)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/spiltbeans/console-dictation/issues"
	},
	"homepage": "https://github.com/spiltbeans/console-dictation#readme",
	"devDependencies": {
		"@types/node": "^20.2.5",
		"typescript": "^5.1.3"
	}
}